plugins:
    - name: 'Myplugin'
      git: 'git://github.com/machimotako/siriproxy-myplugin.git'
      host: '192.168.0.9'
      signals:
        stb:
          power: 3
          change_ch:
            kidsstation: #'Kids station'
              - 30 #CATV
              - 44 #input number
              - 36 #5
              - 32 #1
              - 32 #1
            animax: #'Animax'
              - 30 #CATV
              - 44 #input number
              - 36 #5
              - 32 #1
              - 33 #2
        light:
          power:
            on: 135
            off: 140

